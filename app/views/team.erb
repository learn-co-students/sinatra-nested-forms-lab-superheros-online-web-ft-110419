<h1><%= @team.name %></h1>
 
<div class="team">
  <h3>Team Motto: <%= @team.motto %></h3>
</div><br>
 

<% @members.each do |member| %>
  <div class="course">
    <h3>Hero Name: <%= member.name %></h3><br>
    <p>Hero Power: <%= member.power %></p><br>
    <p>Hero Biography: <%= member.biography %></p><br>
  </div><br>
<% end %>


